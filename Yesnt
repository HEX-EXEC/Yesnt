pcall(function() repeat wait() until game:IsLoaded() local T=workspace:WaitForChild("Titans",5) if not T then return end wait(3.5) local HRP=game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart") local R=game.ReplicatedStorage:WaitForChild("Assets"):WaitForChild("Remotes") for _,v in ipairs(T:GetChildren()) do local n=v:FindFirstChild("Hitboxes") and v.Hitboxes:FindFirstChild("Hit") and v.Hitboxes.Hit:FindFirstChild("Nape") if n then n.ChildAdded:Connect(function(c) if c.Name=="Blood" or c.Name=="Hit" then task.spawn(function() while c.Parent do c:Destroy() wait() end end) end end) task.spawn(function() while n do n.Position=HRP.Position wait() end end) end end R.GET:InvokeServer("S_Skills","Usage","23") wait(3) R.GET:InvokeServer("S_Skills","Usage","14") wait(0.5) R.GET:InvokeServer("Functions","Retry","Add") end)
